{
  "manifest_version": 3,
  "name": "Volume Control",
  "version": "0.1.0",
  "description": "Controls the volume on the active tab and allows you to change it.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_icon": "icons/light_icon128.png",
    "default_title": "Volume Control",
    "default_popup": "popup.html",
    "theme_icons": [
      {
        "light": "icons/light_icon128.png",
        "dark": "icons/dark_icon128.png",
        "size": 128
      }
    ]
  },
  "icons": {
    "128": "icons/light_icon128.png"
  }
}